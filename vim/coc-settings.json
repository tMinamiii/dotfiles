{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.work",
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": false,
        "staticcheck": true
        // "hints": {
        //   "assignVariableTypes": true,
        //   "compositeLiteralFields": true,
        //   "compositeLiteralTypes": true,
        //   "constantValues": true,
        //   "functionTypeParameters": true,
        //   "parameterNames": true,
        //   "rangeVariableTypes": true
        // },
      }
    }
  },
  "json.enable": true,
  "json.format.enable": true,
  "biome.enable": true,
  "biome.requireConfiguration": true,
  "coc.preferences.formatOnSave": true,
  // "eslint.autoFixOnSave": true,
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "CocFloating"
  },
  "inlayHint.display": true,
  "inlayHint.enable": false,
  "inlayHint.enableParameter": true,
  "inlayHint.position": "inline",
  "prettier.enable": true,
  "prettier.requireConfig": false,
  "prettier.formatterPriority": -1,
  "prettier.semi": false,
  "prettier.useTabs": false,
  "prettier.tabWidth": 2,
  "prettier.endOfLine": "auto",
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "as-needed",
  "prettier.printWidth": 120,
  "prettier.singleQuote": true,
  "prettier.arrowParens": "always",
  "prettier.trailingComma": "none",
  "prettier.bracketSpacing": true,
  "stylua.checkUpdate": true,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-17",
      "path": "~/.local/share/mise/installs/java/17"
    },
    {
      "name": "JavaSE-18",
      "path": "~/.local/share/mise/installs/java/18"
    },
    {
      "name": "JavaSE-19",
      "path": "~/.local/share/mise/installs/java/19"
    },
    {
      "name": "JavaSE-20",
      "path": "~/.local/share/mise/installs/java/20"
    },
    {
      "name": "JavaSE-21",
      "path": "~/.local/share/mise/installs/java/21"
    },
  ],
  "suggest.completionItemKindLabels": {
    "class": "󰠱",
    "color": "󰏘",
    "constant": "󰏿",
    "constructor": "",
    "enum": "",
    "enumMember": "",
    "event": "",
    "field": "󰜢",
    "file": "󰈙",
    "folder": "󰉋",
    "function": "󰊕",
    "interface": "",
    "keyword": "󰌋",
    "method": "󰆧",
    "module": "",
    "operator": "󰆕",
    "property": "󰜢",
    "reference": "󰈇",
    "snippet": "",
    "struct": "󰙅",
    "text": "󰉿",
    "typeParameter": "",
    "unit": "󰑭",
    "value": "󰎠",
    "variable": "󰀫",
    "default": "\uf29c",
  }
}
